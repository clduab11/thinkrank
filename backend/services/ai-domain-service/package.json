{
  "name": "@thinkrank/ai-domain-service",
  "version": "2.0.0",
  "description": "Unified AI domain service consolidating content generation, research problems, and detection capabilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest --coverage --testMatch='**/__tests__/**/*.test.ts'",
    "test:watch": "jest --watch",
    "test:unit": "jest --testMatch='**/__tests__/unit/**/*.test.ts'",
    "test:integration": "jest --testMatch='**/__tests__/integration/**/*.test.ts'",
    "test:contract": "pact-mock-service start --port 1234 --host localhost --log pact.log --spec 2",
    "test:tdd": "jest --watch --testMatch='**/__tests__/tdd/**/*.test.ts'",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t thinkrank/ai-domain-service .",
    "docker:run": "docker run -p 3002:3002 thinkrank/ai-domain-service"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.16.1",
    "@supabase/supabase-js": "^2.39.1",
    "@thinkrank/shared": "^1.0.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "mathjs": "^12.0.0",
    "ml-matrix": "^6.10.4",
    "openai": "^4.24.1",
    "pg": "^8.11.3",
    "pino": "^8.17.1",
    "pino-http": "^9.0.0",
    "retry": "^0.13.1",
    "uuid": "^9.0.1",
    "vite": "7.1.7"
  },
  "devDependencies": {
    "@pact-foundation/pact": "^15.0.1",
    "@types/amqplib": "^0.10.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "testcontainers": "^10.2.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.2.0"
  }
}
